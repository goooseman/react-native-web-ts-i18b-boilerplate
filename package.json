{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "prestart": "npm run intl:generate-json",
    "android": "npm run start -- --android",
    "ios": "npm run start -- --ios",
    "web": "npm run start -- --web",
    "lint:check": "npm run lint:prettier:check . && npm run lint:tsc:check && npm run lint:eslint:check .",
    "lint:write": "npm run lint:prettier:write . && npm run lint:eslint:write .",
    "lint:prettier:check": "prettier --check --ignore-path .gitignore",
    "lint:prettier:write": "prettier --write --ignore-path .gitignore",
    "lint:tsc:check": "tsc --noEmit",
    "lint:eslint:check": "eslint --ext .ts,.tsx --ignore-path .gitignore",
    "lint:eslint:write": "eslint --ext .ts,.tsx --ignore-path .gitignore --fix",
    "intl:update-po": "gettext-utils export-strings --default-locale=en-GB && git add src/i18n/*.po*",
    "intl:generate-json": "gettext-utils import-strings"
  },
  "dependencies": {
    "expo": "~39.0.2",
    "expo-status-bar": "~1.0.2",
    "gettext-utils": "^2.2.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.3.tar.gz",
    "react-native-web": "~0.13.12",
    "react-targem": "^1.7.1"
  },
  "devDependencies": {
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "react-native-web": "~0.13.7",
    "typescript": "~3.9.5"
  },
  "private": true
}
